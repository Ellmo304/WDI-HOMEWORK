<h1>Edit this quote</h1>
<form action="/quotes/<%= quote.id %>" method="post">
  <input type="hidden" name="_method" value="put">

  <div class="form-group">
  <label for="quote_title">Quote</label>
  <input type="text" name="quote[quote]" id="quote_quote" class="form-control" value="<%= quote.quote %>">
</div>

<div class="form-group">
  <label for="quote_author">Author</label>
  <input type="text" name="quote[author]" id="quote_author" class="form-control" value="<%= quote.author %>">
</div>

<div class="form-group">
  <label for="quote_year">Year</label>
  <input type="text" name="quote[year]" id="quote_year" class="form-control" value="<%= quote.year %>">
</div>

<button class="btn btn-primary">Update</button>
</form>
